filterParams: {
    lookup: async (keyword) => {
        try {
            console.log('Filter invoked with keyword:', keyword);

            // Fallback if keyword is undefined or null
            const searchTerm = keyword ? keyword.toLowerCase() : '';

            // Ensure conf has valid data to filter
            const filteredData = conf.filter(item =>
                item.ClientName?.toLowerCase().includes(searchTerm)
            );

            // Return the options in the format expected by the filter component
            return filteredData.map(data => ({
                label: data.ClientName,
                value: data.ClientName,
            }));
        } catch (error) {
            console.error('Error during filtering:', error);
            return [];
        }
    },
},
